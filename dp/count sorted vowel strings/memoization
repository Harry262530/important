class Solution {
public:

    int helper(int ind,int last,string temp,int n,vector<char> &v,vector<vector<int>> &dp){
        if (ind<0 && temp.size()==n){
            return 1;
        }
        if (dp[ind][last]!=-1){
            return dp[ind][last];
        }
        int count=0;
        for (int i=0;i<5;i++){
            if (temp.empty() || temp[temp.size()-1]<=v[i]){
                count+=helper(ind-1,i,temp+v[i],n,v,dp);
            }  
        }
        return dp[ind][last]=count;
    }

    int countVowelStrings(int n) {
        vector<char> v{'a','e','i','o','u'};
        vector<vector<int>> dp(n,vector<int> (5,-1));
        return helper(n-1,0,"",n,v,dp);
    }
};
