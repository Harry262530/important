int lcs(string &str1, string &str2){
    int n1=str1.size();
    int n2=str2.size();
    vector<int> prev(n2+1,0);
    vector<int> temp(n2+1,0); 
    int ans=0;
    for (int i=1;i<=n1;i++){
        for (int j=1;j<=n2;j++){
            if (str1[i-1]==str2[j-1]){
                temp[j]=1+prev[j-1];
                ans=max(ans,temp[j]);
            }
            else{
                temp[j]=0;
            }
        }
        prev=temp;
    }
    return ans;

}
