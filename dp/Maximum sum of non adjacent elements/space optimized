#include <bits/stdc++.h> 
int maximumNonAdjacentSum(vector<int> &nums){
    int n=nums.size();
    int prev1=nums[0];
    int prev2;
    for (int i=1;i<n;i++){
        int pick=nums[i];
        if (i>1){
            pick+=prev2;
        }
        int not_pick=prev1;
        int curr=max(pick,not_pick);
        prev2=prev1;
        prev1=curr;
    }
    return prev1;
}
