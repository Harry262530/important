int cutRod(vector<int> &price, int n)
{

	vector<vector<int>> dp(n,vector<int>(n+1,0));
	// return helper(n-1,n,dp,price);
	for (int i=0;i<n+1;i++){
		dp[0][i]=i*price[0];
	}
	for (int i=1;i<n;i++){
		for (int j=0;j<=n;j++){
			int notpick=dp[i-1][j];
			int rodl=i+1;
			int pick=INT_MIN;
			if (rodl<=j){
				pick=price[i]+dp[i][j-rodl];
			}
			dp[i][j]=max(pick,notpick);
		}
	}
	return dp[n-1][n];
}
